{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "consistent-return": "off",
        "radix":"off",
        "no-param-reassign": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off"
    }
}